{
  "name": "oa42-opentelemetry",
  "version": "0.1.4",
  "description": "",
  "type": "module",
  "main": "./out-commonjs/main.js",
  "module": "./out/main.js",
  "types": "./out/main.d.ts",
  "browser": "./out/browser.js",
  "exports": {
    ".": {
      "require": "./out-commonjs/main.js",
      "import": "./out/main.js",
      "types": "./out/main.d.ts",
      "browser": "./out/browser.js"
    }
  },
  "files": [
    "./out/**",
    "./out-commonjs/**"
  ],
  "scripts": {
    "watch": "tsc --watch",
    "prepare": "tsc ; tsc --outDir out-commonjs --declaration false --module commonjs --moduleResolution Node10 ; echo '{\"type\":\"commonjs\"}' > out-commonjs/package.json",
    "clean": "rm -rf out out-*",
    "test": "node --test ./out/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:LuvDaSun/OpenApi42"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuvDaSun/OpenApi42/issues"
  },
  "keywords": [
    "openapi",
    "opentelemetry"
  ],
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/instrumentation": "^0.48.0",
    "@types/node": "^20.11.19",
    "oa42-lib": "^0.8.10",
    "tslib": "^2.6.2",
    "type-fest": "^4.10.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "typescript": "^5.3.3"
  }
}
