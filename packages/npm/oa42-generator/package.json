{
  "name": "oa42-generator",
  "version": "0.8.8",
  "sideEffects": false,
  "description": "",
  "type": "module",
  "main": "./out-commonjs/main.js",
  "module": "./out/main.js",
  "types": "./out/main.d.ts",
  "exports": {
    ".": {
      "require": "./out-commonjs/main.js",
      "import": "./out/main.js",
      "types": "./out/main.d.ts"
    }
  },
  "files": [
    "./out/**",
    "./out-commonjs/**",
    "./bin/**"
  ],
  "bin": {
    "oa42-generator": "bin/oa42-generator"
  },
  "scripts": {
    "watch": "tsc --watch",
    "prepare": "tsc ; tsc --outDir out-commonjs --declaration false --module commonjs --moduleResolution Node10 ; echo '{\"type\":\"commonjs\"}' > out-commonjs/package.json",
    "clean": "rm -rf out out-*",
    "test": "node --test ./out/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LuvDaSun/OpenApi42"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuvDaSun/OpenApi42/issues"
  },
  "keywords": [
    "openapi",
    "generator"
  ],
  "dependencies": {
    "@jns42/oas-v3-0": "^0.13.6",
    "@jns42/oas-v3-1": "^0.13.6",
    "@jns42/swagger-v2": "^0.13.6",
    "@types/node": "^20.11.17",
    "@types/yargs": "^17.0.32",
    "camelcase": "^8.0.0",
    "goodrouter": "^2.1.3",
    "jns42-generator": "^0.13.6",
    "oa42-lib": "^0.8.8",
    "tslib": "^2.6.2",
    "type-fest": "^4.10.2",
    "typescript": "^5.3.3",
    "yaml": "^2.3.4",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "typescript": "^5.3.3"
  }
}
